<div class ='row'>
<div class="col s10 offset-s1">
	<%= form_for @property do |f| %>
		<%= f.label :street %>
		<%= f.text_field :street, class:"validate" %>
	<br />
		<%= f.label :city %>
		<%= f.text_field :city %>
	<br />
		<%= f.label :state %>
		<%= f.text_field :state %>

	<br />
		<%= f.label :zip %>
		<%= f.text_field :zip %>
	<br />
		<%= f.label :price %>
		<%= f.number_field :price %>
	<br />
		<%= f.label :status %>
		<%= f.select :status, [['for sale', 1], ['sold', 2]], class:"browser-default" %>
  <br />
		<%= f.submit %>
	<% end %>
	</div></div>
